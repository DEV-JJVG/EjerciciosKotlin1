fun main() {
    val numero = 7
    esPrimo(numero)
} 
fun esPrimo(numero: Int){
     if (numero < 2) {
        println("El número $numero no es primo")
     }
    for (i in 2..kotlin.math.sqrt(numero.toDouble()).toInt()) {
        if (numero % i == 0){
             println("El número $numero no es primo")
        }
    }
    else{
        println("El número $numero es primo")
    }
} 
